{
  "name": "oerwi",
  "version": "0.6.3",
  "dependencies": {
    "acorn": "^7.1.1",
    "express": "4.17.1",
    "handlebars": "^4.3.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "deploy-development": "docker build --tag shroomlife/oerwi:development . && docker push shroomlife/oerwi:development",
    "deploy-production": "docker build --tag shroomlife/oerwi:latest . && docker push shroomlife/oerwi:latest"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/helper-call-delegate": "^7.11.4",
    "@rocketseat/unform": "1.5.1",
    "@svgr/webpack": "4.3.2",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "0.3.3",
    "babel-preset-react-app": "9.0.1",
    "bootstrap": "4.3.1",
    "camelcase": "5.3.1",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "css-loader": "3.2.0",
    "dotenv": "8.1.0",
    "dotenv-expand": "5.1.0",
    "eslint": "6.2.2",
    "eslint-config-react-app": "5.2.0",
    "eslint-loader": "3.0.0",
    "eslint-plugin-flowtype": "4.2.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "2.0.1",
    "file-loader": "4.2.0",
    "fs-extra": "8.1.0",
    "hex-to-rgba": "2.0.1",
    "html-webpack-plugin": "4.0.0-beta.8",
    "i18next": "17.0.12",
    "i18next-browser-languagedetector": "3.0.3",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "2.1.0",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.0",
    "jquery": "3.5.1",
    "mini-css-extract-plugin": "0.8.0",
    "moment": "2.24.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.5.0",
    "popper.js": "1.15.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "randomcolor": "0.5.4",
    "react": "16.12.0",
    "react-app-polyfill": "1.0.6",
    "react-block-ui": "1.3.2",
    "react-dev-utils": "10.2.0",
    "react-dom": "16.12.0",
    "react-i18next": "11.3.3",
    "react-icons": "3.9.0",
    "react-router-dom": "5.1.2",
    "react-scripts": "^3.4.0",
    "react-social-icons": "4.1.0",
    "react-toastify": "5.5.0",
    "resolve": "1.15.1",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "8.0.2",
    "semver": "6.3.0",
    "style-loader": "1.0.0",
    "sweetalert2": "9.8.2",
    "terser-webpack-plugin": "1.4.1",
    "ts-pnp": "1.1.2",
    "url-loader": "2.1.0",
    "uuid": "3.3.3",
    "webpack": "4.41.6",
    "webpack-dev-server": "3.10.3",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "5.0.0"
  },
  "resolutions": {
    "serialize-javascript": "^3.1.0",
    "handlebars": "^4.3.0",
    "react-scripts": "^3.4.0",
    "minimist": "^0.2.1",
    "acorn": "^7.1.1",
    "kind-of": "^6.0.3",
    "websocket-extensions": "^0.1.4",
    "lodash": "^4.17.19",
    "elliptic": "^6.5.3",
    "http-proxy": "^1.18.1",
    "yargs-parser": "^13.1.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "license": "Apache-2.0"
}
